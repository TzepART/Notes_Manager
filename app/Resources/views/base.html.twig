<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Symfony Demo application{% endblock %}</title>

    {% block stylesheets %}

        <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset('style.css') }}">
        <link rel="stylesheet" href="{{ asset('css/additional_style.css') }}">

    {% endblock %}
</head>

<body id="{% block body_id %}{% endblock %}">

{% block header %}
    <div class="header clearfix">
        <nav>
            <ul class="nav nav-pills pull-right">
                <li role="presentation"><a href="list_circle.html">Home</a></li>
                <li role="presentation"><a href="create_circle.html">Добавить окружность</a></li>
                <li role="presentation"><a href="create_note.html">Добавить заметку</a></li>
                <li role="presentation"><a href="index.html">About</a></li>
            </ul>
        </nav>
        <h3 class="text-muted">Notes Manager <span class="label label-default">Beta</span></h3>
    </div>

{% endblock %}

<div class="container body-container">
    {% block main %}{% endblock %}
</div>

{% block footer %}
    <footer class="footer">
        <p>&copy; 2016 TzepArt Company, Inc.</p>
    </footer>
{% endblock %}

{% block javascripts %}

    <script src="{{ asset('js/jcanvas.js') }}"></script>
    <script src="{{ asset('js/script.js') }}"></script>
{% endblock %}

</body>
</html>
