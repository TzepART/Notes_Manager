                <td>{{ note.text }}</td>
                <td>{% if note.dateCreate %}{{ note.dateCreate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if note.dateUpdate %}{{ note.dateUpdate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('notes_show', { 'id': note.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('notes_edit', { 'id': note.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
